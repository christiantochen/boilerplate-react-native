{
  "name": "react-native-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "react-native run-android --variant=devDebug --appIdSuffix=dev.debug",
    "staging": "react-native run-android --variant=devRelease --appIdSuffix=dev",
    "release": "react-native run-android --variant=prodRelease",
    "start": "npm run dev & npm run dev:tailwind",
    "test": "jest",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.16.0",
    "@react-native-community/datetimepicker": "^5.1.0",
    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-config": "^1.4.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.11.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.0.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "postcss": "^8.4.6",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.19"
  },
  "jest": {
    "preset": "react-native"
  }
}
